{
  "name": "githubist-web",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run clear:dev && cross-env NODE_ENV=development node scripts/start.js",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "test": "TZ=Europe/Istanbul node scripts/test.js",
    "coverage": "TZ=Europe/Istanbul node scripts/test.js --coverage",
    "lint:js": "eslint .",
    "lint:flow": "flow check",
    "lint:css": "stylelint 'src/**/*.scss'",
    "lint": "yarn run lint:js && yarn run lint:flow && yarn run lint:css",
    "format:js": "prettier --write 'src/**/*.{js,jsx}' 'scripts/**/*.js' 'server/**/*.js' 'webpack/**/*.js'",
    "format:css": "prettier-stylelint --quiet --write 'src/**/*.scss'",
    "format": "yarn run format:js && yarn run format:css",
    "flow-typed-install": "flow-typed install",
    "clear:dev": "rimraf dist",
    "clear:prod": "rimraf build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "precommit": "lint-staged",
    "now-start": "node build/server.js"
  },
  "dependencies": {
    "apollo-boost": "^0.1.16",
    "apollo-link-http": "^1.5.5",
    "classnames": "^2.2.6",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "helmet": "^3.13.0",
    "node-fetch": "^2.2.0",
    "number-abbreviate": "^2.0.0",
    "react": "^16.5.2",
    "react-apollo": "^2.2.4",
    "react-autosuggest": "^9.4.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.11",
    "react-router-dom": "4.2.2",
    "react-universal-component": "^3.0.3",
    "waait": "^1.0.2",
    "webpack-flush-chunks": "^2.0.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.14",
    "@storybook/addon-backgrounds": "^3.4.8",
    "@storybook/addon-links": "^4.0.0-alpha.14",
    "@storybook/addons": "^4.0.0-alpha.14",
    "@storybook/react": "^4.0.0-alpha.14",
    "autoprefixer": "^9.1.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-universal-import": "^3.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "dotenv-webpack": "^1.5.7",
    "empty": "^0.10.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-eslint-comments": "^3.0.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-graphql": "^2.1.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "extract-css-chunks-webpack-plugin": "^3.0.11",
    "flow-bin": "^0.82.0",
    "flow-typed": "^2.5.1",
    "fs-extra": "^7.0.0",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.13.7",
    "prettier-stylelint": "^0.4.2",
    "react-dev-utils": "^5.0.2",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "stats-webpack-plugin": "^0.6.2",
    "style-loader": "^0.22.1",
    "stylelint": "^9.5.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-sass-guidelines": "^5.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^1.1.0",
    "stylelint-declaration-strict-value": "^1.0.4",
    "stylelint-order": "^1.0.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.1.1",
    "webpack": "4.16.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-merge": "^4.1.3",
    "write-file-webpack-plugin": "^4.3.2"
  },
  "jest": {
    "transform": {
      ".(gql|graphql)": "jest-transform-graphql",
      ".(js|jsx)": "babel-jest"
    },
    "testRegex": "(\\.(test|spec))\\.(jsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.cache/"
    ],
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "\\.(scss|svg|png|jpg)$": "<rootDir>/src/styleMock.js"
    },
    "moduleFileExtensions": [
      "jsx",
      "js"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ],
    "setupTestFrameworkScriptFile": "./src/setupTests.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}",
      "!src/components/**/index.{js,jsx}",
      "!src/components/**/*.stories.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 72,
        "functions": 72,
        "lines": 72,
        "statements": 72
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn run format:js",
      "git add"
    ],
    "*.scss": [
      "yarn run format:css",
      "git add"
    ]
  }
}
